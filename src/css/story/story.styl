  .slider-wrapper
    position absolute
    bottom -24px
    width 100%
    height 20px
    .play-pause
      width 30px
      transform translate(-100%,-50%)
      position absolute
      top 6px
      cursor pointer
      cursor hand
      left -5px
      .pause-button
        width 100%
        margin-top 4px
        display none
        rect
          stroke none
          fill white
      .play-button
        width 100%
        display block
        margin-top 5px
        polygon
          fill white
          stroke none
    #slider
      background none
      border none
      box-shadow none
      .noUi-target
        background none
      .noUi-handle
        height 20px
        width 20px
        border-radius 50%
        cursor hand
        cursor pointer
        outline none
        box-shadow none
        &:before, &:after
          display none
      .noUi-base
        background none
        .noUi-origin
          top 5px
        &:after
          content ''
          width 100%
          height 5px
          position absolute
          top 6px
          border-radius 0px
          background rgba(255,255,255,.2)
      .noUi-connects
        border-radius 0px
        .noUi-connect
          background white
          height 5px
          border-radius 0px
          margin-top 6px
      .noUi-tooltip
        background none
        border none
        font-family 'National 2 Narrow Web'
        color #8e8e8e
        font-size 14px
        font-weight 500
        -webkit-font-smoothing auto
        text-transform uppercase
      .noUi-value
        font-family 'National 2 Narrow Web'
        font-weight 600
        color white
        font-size 16px
        margin-top 0px
        padding 5px 0px
        transform translate(calc(-50% + 8px),0)
